{
  "compilerOptions": {
    "importHelpers": true,
    "sourceMap": true,
    "inlineSources": true,
    "noEmitHelpers": false,
    "noImplicitAny": true,
    "declaration": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "jasmine",
      "webpack"
    ],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "./temp/out-tsc",
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
